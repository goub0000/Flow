# Supabase Configuration (REQUIRED - Primary Database)
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_service_role_key

# CORS Configuration (REQUIRED for Railway deployment)
# Add your Railway domain and any frontend domains
ALLOWED_ORIGINS=http://localhost:8080,https://your-railway-app.railway.app,https://your-frontend-domain.com

# Optional: Data API Keys (only needed for data enrichment scripts)
COLLEGE_SCORECARD_API_KEY=your_api_key_here
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_key

# Optional: Rate limiting
COLLEGE_SCORECARD_RATE_LIMIT_DELAY=0.1
